{
  "name": "lemmih",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.20.0",
  "repository": "github:insel-null/lemmih",
  "bugs": "https://github.com/insel-null/lemmih/issues",
  "keywords": [
    "bun",
    "http",
    "web",
    "server",
    "router"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bun": {
      "types": "./dist/bun.d.ts",
      "default": "./dist/bun.js"
    },
    "./cors": {
      "types": "./dist/layers/cors.d.ts",
      "default": "./dist/layers/cors.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bench:frameworks": "bun run ./bench/frameworks/bench.ts",
    "bench:frameworks-init": "bun run ./bench/frameworks-init/bench.ts",
    "bench:kill-port": "nix run nixpkgs#killport -- 3000",
    "bench:routers": "bun run ./bench/routers/bench.ts",
    "build": "pkgroll --clean-dist",
    "bump": "bumpp --no-push",
    "dev": "bun run ./test/fixtures/example.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "cors-edge": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@moeru/eslint-config": "^0.1.0-beta.13",
    "@types/bun": "latest",
    "@types/kill-port": "^2.0.3",
    "bumpp": "^10.3.1",
    "bun": "^1.3.3",
    "elysia": "^1.4.16",
    "eslint": "^9.38.0",
    "hono": "^4.10.6",
    "kill-port": "^2.0.1",
    "memoirist": "^0.4.0",
    "mitata": "^1.0.34",
    "pkgroll": "^2.21.1",
    "typescript": "^5"
  }
}
